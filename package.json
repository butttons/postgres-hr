{
    "name": "postgres-hr",
    "version": "0.0.21",
    "description": "Manage Postgres roles in a sane way",
    "main": "build/server.js",
    "scripts": {
        "dev": "PORT=9876 nodemon build/server.js --ignore 'postgres-hr.json'",
        "tsc:w": "tsc --watch",
        "build": "tsc",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepack": "npm run build"
    },
    "bin": {
        "postgres-hr": "./build/server.js"
    },
    "keywords": [
        "postgres",
        "postgresql",
        "roles",
        "role management",
        "gui",
        "rbac"
    ],
    "author": "butttons",
    "license": "MIT",
    "dependencies": {
        "body-parser": "^1.19.0",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "knex": "^2.4.0",
        "lodash.isequal": "^4.5.0",
        "lowdb": "^1.0.0",
        "module-alias": "^2.2.0",
        "pg": "^8.5.1",
        "platform-folders": "^0.5.2",
        "reflect-metadata": "^0.1.10",
        "shortid": "^2.2.15"
    },
    "devDependencies": {
        "@types/express": "^4.17.1",
        "@types/lodash": "^4.14.141",
        "@types/lodash.isequal": "^4.5.5",
        "@types/lowdb": "^1.0.9",
        "@types/node": "^8.0.29",
        "@types/pg": "^7.14.9",
        "typescript": "^3.5.3"
    },
    "_moduleAliases": {
        "@": "build"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/butttons/postgres-hr"
    }
